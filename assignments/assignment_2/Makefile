# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Problem2_1_config = debug
  Problem2_2_config = debug
endif
ifeq ($(config),release)
  Problem2_1_config = release
  Problem2_2_config = release
endif

PROJECTS := Problem2_1 Problem2_2

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Problem2_1:
ifneq (,$(Problem2_1_config))
	@echo "==== Building Problem2_1 ($(Problem2_1_config)) ===="
	@${MAKE} --no-print-directory -C . -f Problem2_1.make config=$(Problem2_1_config)
endif

Problem2_2:
ifneq (,$(Problem2_2_config))
	@echo "==== Building Problem2_2 ($(Problem2_2_config)) ===="
	@${MAKE} --no-print-directory -C . -f Problem2_2.make config=$(Problem2_2_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Problem2_1.make clean
	@${MAKE} --no-print-directory -C . -f Problem2_2.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Problem2_1"
	@echo "   Problem2_2"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"