# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Fibonacci_config = debug
  DoChange_config = debug
endif
ifeq ($(config),release)
  Fibonacci_config = release
  DoChange_config = release
endif

PROJECTS := Fibonacci DoChange

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Fibonacci:
ifneq (,$(Fibonacci_config))
	@echo "==== Building Fibonacci ($(Fibonacci_config)) ===="
	@${MAKE} --no-print-directory -C . -f Fibonacci.make config=$(Fibonacci_config)
endif

DoChange:
ifneq (,$(DoChange_config))
	@echo "==== Building DoChange ($(DoChange_config)) ===="
	@${MAKE} --no-print-directory -C . -f DoChange.make config=$(DoChange_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Fibonacci.make clean
	@${MAKE} --no-print-directory -C . -f DoChange.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Fibonacci"
	@echo "   DoChange"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"