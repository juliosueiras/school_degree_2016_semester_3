# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Midterm_config = debug
endif
ifeq ($(config),release)
  Midterm_config = release
endif

PROJECTS := Midterm

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Midterm:
ifneq (,$(Midterm_config))
	@echo "==== Building Midterm ($(Midterm_config)) ===="
	@${MAKE} --no-print-directory -C . -f Midterm.make config=$(Midterm_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Midterm.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Midterm"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"